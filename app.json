{
	"id": "nl.alecto",
	"version": "1.1.0",
	"compatibility": ">=0.9",
	"category": "appliances",
	"name": {
		"en": "Alecto",
		"nl": "Alecto"
	},
	"description": {
		"en": "Give Homey the ability to communicate with Alecto devices",
		"nl": "Laat Homey met Alecto devices praten"
	},
	"author": {
		"name": "Athom B.V."
	},
	"images": {
		"large": "./assets/images/large.png",
		"small": "./assets/images/small.png"
	},
	"permissions": [
		"homey:wireless:433"
	],
	"flow": {
		"actions": [
			{
				"id": "triggerDoorbell",
				"title": {
					"en": "Ring the doorbell",
					"nl": "Laat de deurbel af gaan"
				},
				"args": [
					{
						"name": "device",
						"type": "device",
						"filter": "driver_id=doorbell",
						"placeholder": {
							"en": "Which doorbell?",
							"nl": "Welke deurbel?"
						}
					}
				]
			}
		]
	},
